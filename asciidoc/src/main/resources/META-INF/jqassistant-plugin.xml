<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="Asciidoc">
    <description>Provides a scanner for Asciidoc documents.</description>
    <model>
        <class>de.kontext_e.jqassistant.plugin.asciidoc.store.descriptor.AsciidocDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.asciidoc.store.descriptor.AsciidocFileDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.asciidoc.store.descriptor.AsciidocBlockDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.asciidoc.store.descriptor.AsciidocTableDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.asciidoc.store.descriptor.AsciidocTableRowDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.asciidoc.store.descriptor.AsciidocTableCellDescriptor</class>
    </model>
    <scanner>
        <class>de.kontext_e.jqassistant.plugin.asciidoc.scanner.AsciidocFileScannerPlugin</class>
    </scanner>
  <rules>
      <resource>asciidoc.xml</resource>
  </rules>
</jqa-plugin:jqassistant-plugin>
