<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="Checkstyle">
    <description>Provides a scanner for Graphviz dot files.</description>
    <model>
        <class>de.kontext_e.jqassistant.plugin.dot.store.descriptor.DotFileDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.dot.store.descriptor.DotGraphDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.dot.store.descriptor.DotNodeDescriptor</class>
    </model>
    <scanner>
        <class>de.kontext_e.jqassistant.plugin.dot.scanner.DotScannerPlugin</class>
    </scanner>

</jqa-plugin:jqassistant-plugin>
